<!DOCTYPE html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js"> <!--<![endif]-->
<html>
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    <title>{% block title %}Template Landing Bundle{% endblock %}</title>
    <link href='https://fonts.googleapis.com/css?family=Asap' rel='stylesheet' type='text/css'>
    {% block css %}
	   	{% stylesheets
	   		'@SopinetTemplateLandingBundle/Resources/public/css/bootstrap.css'
	    	'bundles/sopinettemplatelanding/css/font-awesome.css'
	    	'@SopinetTemplateLandingBundle/Resources/public/css/style.css'
	    	output='css/*.css' filter='cssrewrite' %}
	    	<link rel="stylesheet" type="text/css" media="screen" href="{{ asset_url }}" />
		{% endstylesheets %}
    {% endblock %}

</head>
	{% block custombody %}<body class="1">{% endblock %}
	{% block body %}
	    <h1>Hello, world!</h1>
	{% endblock %}

{% block js %}
	{% javascripts 
		'@SopinetTemplateLandingBundle/Resources/public/js/jquery.js'
		'@SopinetTemplateLandingBundle/Resources/public/js/jquery-ui.js'
		'@SopinetTemplateLandingBundle/Resources/public/js/bootstrap.min.js'
		'@SopinetTemplateLandingBundle/Resources/public/js/fancybox/jquery.fancybox.pack.js'
		'@SopinetTemplateLandingBundle/Resources/public/js/jquery.tags-cloud.min.js'
		'@SopinetTemplateLandingBundle/Resources/public/js/custom.js'
	%}
	<script type="text/javascript" src="{{ asset_url }}"></script>
	{% endjavascripts %}
{% endblock %}
    
</body>
</html>
